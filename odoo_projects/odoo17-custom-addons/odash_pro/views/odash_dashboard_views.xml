<odoo>
    <record id="view_odash_dashboard_access_tree" model="ir.ui.view">
        <field name="name">odash_pro.dashboard.tree</field>
        <field name="model">odash_pro.dashboard</field>
        <field name="arch" type="xml">
            <tree string="Accesses" create="0">
                <field name="user_id" />
                <field name="page_id" />
                <field name="allowed_company_ids" widget="many2many_tags" />
                <field name="last_authentication_date"/>
            </tree>
        </field>
    </record>

     <record id="action_odash_dashboard_accesses" model="ir.actions.act_window">
        <field name="name">Pages</field>
        <field name="res_model">odash_pro.dashboard</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
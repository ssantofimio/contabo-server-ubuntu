<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_change_effective_date_wizard" model="ir.ui.view">
    <field name="name">change.effective.date.wizard.form</field>
    <field name="model">change.effective.date.wizard</field>
    <field name="arch" type="xml">
      <form string="Change Effective Date">
        <group>
          <field name="new_effective_date"/>
        </group>
        <footer>
          <button name="apply_new_effective_date" type="object" string="Apply" class="btn-primary"/>
          <button string="Cancel" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
